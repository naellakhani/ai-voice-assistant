# Copy this file to .env.docker and fill in your actual values

GOOGLE_APPLICATION_CREDENTIALS_SPEECH=
GOOGLE_APPLICATION_CREDENTIALS_TEXT=

# Google Gemini API key 
GOOGLE_API_KEY=

# ===========================================
# TWILIO (Voice Services)
# ===========================================
# Your Twilio Account SID
TWILIO_ACCOUNT_SID=
# Your Twilio Auth Token
TWILIO_AUTH_TOKEN=
# Twilio phone number for outbound calls (format: +1234567890)
TWILIO_FROM_NUMBER=

# Format: postgresql://username:password@host:port/database
DATABASE_URL=

# Database password for Docker Compose (used in docker-compose.yml)
DB_PASSWORD=

# TTS Provider: "elevenlabs" or "google" or "cartesia"
TTS_PROVIDER=

# ElevenLabs API key (if using ElevenLabs TTS)
ELEVENLABS_API_KEY=

# ElevenLabs voice ID (if using ElevenLabs TTS)
ELEVENLABS_VOICE_ID=

# Cartesia API key (if using Cartesia TTS)
CARTESIA_API_KEY=

# Cartesia voice ID (if using Cartesia TTS)
CARTESIA_VOICE_ID=

# Ngrok auth token for creating public tunnels
NGROK_AUTH_TOKEN=

# Application port
PORT=

# ===========================================
# OUTBOUND CALLING
# ===========================================
# Enable outbound calling features: "true" or "false"
ENABLE_OUTBOUND=false

# ===========================================
# EMAIL NOTIFICATIONS
# ===========================================
# SMTP server hostname
SMTP_HOST=smtp.gmail.com

# SMTP server port
SMTP_PORT=587

# SMTP username (usually your email)
SMTP_USERNAME=

# SMTP password (or app password for Gmail)
SMTP_PASSWORD=

# Email address for sending notifications
SMTP_FROM_EMAIL=

# Email address for receiving notifications
SMTP_TO_EMAIL=

# ===========================================
# CRM INTEGRATIONS
# ===========================================
# Primary CRM system: "followupboss", "hubspot", or "zoho"
PRIMARY_CRM=

# Enable CRM integration: "true" or "false"
CRM_ENABLED=false

# --- HubSpot CRM ---
# HubSpot Private App API key
HUBSPOT_API_KEY=

# --- FollowUpBoss CRM ---
# FollowUpBoss API key
FOLLOWUPBOSS_API_KEY=

# FollowUpBoss webhook system identifier (optional)
FOLLOWUPBOSS_X_SYSTEM=

# FollowUpBoss webhook system key (optional)
FOLLOWUPBOSS_X_SYSTEM_KEY=

# --- Zoho CRM ---
# Zoho OAuth Client ID
ZOHO_CLIENT_ID=

# Zoho OAuth Client Secret
ZOHO_CLIENT_SECRET=

# Zoho OAuth Refresh Token
ZOHO_REFRESH_TOKEN=

# Zoho Access Token
ZOHO_ACCESS_TOKEN=

# Zoho region: "ca" (Canada), "eu" (Europe), "in" (India), or "com" (US)
ZOHO_REGION=ca